    <script type="text/javascript" src="//assets.adobedtm.com/926659a1689cc9fca96760b67cca714ae48f0584/satelliteLib-a9d46808a35b7b8034adfab1d8207407222304d5.js">
    </script>
    <link rel="preload" href="/sites/all/themes/shared/science_jcore/library/css/style-jcore.css?p0x8cp" as="style" onload="this.rel='stylesheet'"/>
    <link rel="preload" href="https://fonts.googleapis.com/css?family=Roboto+Condensed:400,400i,700,700i|Roboto:400,400i,700&amp;subset=cyrillic,cyrillic-ext,greek,greek-ext,latin-ext,vietnamese" as="style" onload="this.rel='stylesheet'"/>
