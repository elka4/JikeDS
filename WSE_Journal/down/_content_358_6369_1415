                        <div class="pane-content">
                          <div class="panel-pane pane-highwire-share-link highwire_clipboard_link_ajax" id="shareit">
                            <div class="pane-content">
                              <a href="/" class="link-icon">
                                <i class="fa fa-lg fa-share">
                                </i>
                                <span class="title">
