    <meta name="DC.Title" content="Satellites trace Afghanistan's lost empires"/>
    <meta name="DC.Identifier" content="10.1126/science.358.6369.1364"/>
    <meta name="DC.Date" content="2017-12-15"/>
    <meta name="DC.Publisher" content="American Association for the Advancement of Science"/>
